  "keywords": [
    "stimulus",
    "stimulusjs",
    "stimulus controller",
    "audio",
    "sound"
  ],
  "repository": "git@github.com:stimulus-components/stimulus-sound.git",
  "bugs": {
    "url": "https://github.com/stimulus-components/stimulus-sound/issues"
  },
  "author": "Guillaume Briday <guillaumebriday@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/stimulus-components/stimulus-sound",
  "private": false,
  "main": "dist/stimulus-sound.umd.js",
  "module": "dist/stimulus-sound.mjs",
  "scripts": {
    "lint": "eslint --ext .js,.mjs,.ts . && prettier . --check",
    "lintfix": "eslint --ext .js,.mjs,.ts . --fix && prettier . --write",
    "dev": "vite",
    "prod": "vite build --mode netlify",
